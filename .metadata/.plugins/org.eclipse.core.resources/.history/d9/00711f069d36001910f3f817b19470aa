package com.iadtec.hackathon.pais;

public class Pais {
	
	public class City {

	    private Long id;
	    private String nome;

	    public City() {
	    }

	    public City(Long id, String nome) {
	        this.id = id;
	        this.nome = nome;
	    }

	    public Long getId() {
	        return id;
	    }

	    public void setId(Long id) {
	        this.id = id;
	    }

	    public String getNome() {
	        return nome;
	    }

	    public void setNome(String nome) {
	        this.nome = nome;
	    }

	    @Override
	    public String toString() {
	        return "Pais{" + "id=" + id + ", nome=" + nome + "}";
	    }
	}

}
